//Example
// Connect to MongoDB
mongoose.connect('mongodb://localhost:27017/doctor_db', {
    useNewUrlParser: true,
    useUnifiedTopology: true
}).then(() => {
    console.log('Connected to MongoDB');
}).catch(err => {
    console.error('Error connecting to MongoDB:', err);
});
